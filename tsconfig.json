{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "screen/home.js",
    "screen/reviewdetails.js",
    "routes/note.js",
    "screen/Notescreen.js",
    "app/(tabs)/index.js"
  ]
}
